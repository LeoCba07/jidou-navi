{
  "expo": {
    "name": "JidouNavi",
    "slug": "jidou-navi",
    "scheme": "jidounavi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF3E7"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "JidouNavi needs your location to show nearby vending machines.",
        "NSCameraUsageDescription": "JidouNavi needs camera access to take photos of vending machines.",
        "NSPhotoLibraryUsageDescription": "JidouNavi needs photo library access to select vending machine photos."
      },
      "bundleIdentifier": "com.jidounavi.app"
    },
    "android": {
      "package": "com.jidounavi.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@rnmapbox/maps",
      "expo-router",
      "expo-image-picker",
      "expo-font",
      "expo-localization",
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "organization": "placeholder",
          "project": "placeholder",
          "url": "https://sentry.io/",
          "enableAutoUploadSourceMaps": false
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "JidouNavi needs your location to show nearby vending machines and allow check-ins."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "af5ac775-17e4-43e3-984a-6218423c6bb3"
      }
    },
    "owner": "jidou-navi-organization"
  }
}
